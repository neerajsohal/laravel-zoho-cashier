{
    "name": "msonowal/laravel-zoho-cashier",
    "description": "Laravel Cashier inspired Zoho Cashier provides an expressive, fluent interface to Zohos's subscription billing services.",
    "keywords": ["laravel", "zoho", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/msonowal/laravel-zoho-cashier/issues",
        "source": "https://github.com/msonowal/laravel-zoho-cashier"
    },
    "authors": [
        {
            "name": "Manash Jyoti Sonowal",
            "email": "manash149@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "dompdf/dompdf": "^0.8.3",
        "illuminate/database": "^5.8",
        "illuminate/support": "^5.8",
        "nesbot/carbon": "^2.16",
        "symfony/http-kernel": "^4.2"
    },
    "require-dev": {
        "illuminate/http": "^5.8",
        "illuminate/routing": "^5.8",
        "illuminate/view": "^5.8",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Msonowal\\Laravel\\Zoho\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Msonowal\\Laravel\\Zoho\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Msonowal\\Laravel\\Zoho\\Cashier\\ZohoCashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
